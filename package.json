{
	"name": "vscode-units",
	"publisher": "yasinkuyu",
	"displayName": "VSCode Units",
	"description": "VSCode Inline Units Conversion Extension (px to em /em to px etc.)",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"icon": "icon.png",
	"main": "./extension.js",
	"license": "MIT",
	"repository": "https://github.com/yasinkuyu/vscode-units",
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "VsCodeUnits",
			"properties": {
				"vscodeunits.endianness": {
					"type": "boolean",
					"default": true
				}
			}
		},
		"languages": [
			{
				"id": "vscodeunits"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.39.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
